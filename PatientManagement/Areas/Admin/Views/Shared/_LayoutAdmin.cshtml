    <!--Masked Input [ OPTIONAL ]-->
    <script src="@baseURL/plugins/masked-input/jquery.maskedinput.min.js"></script>

    <!--BootstrapJS [ RECOMMENDED ]-->
    <script src="@baseURL/plugins/ckeditor/ckeditor.js"></script>

    <!--BootstrapJS [ RECOMMENDED ]-->
    <script src="@baseURL/plugins/ckfinder/ckfinder.js"></script>

    <!--NiftyJS [ RECOMMENDED ]-->
    <script src="@baseURL/js/nifty.min.js"></script>

    <!--Bootstrap Validator [ OPTIONAL ]-->
    <script src="@baseURL/plugins/bootstrap-validator/bootstrapValidator.min.js"></script>

    <!--Bootstrap Validator [ OPTIONAL ]-->
    <script src="@baseURL/plugins/bootbox/bootbox.min.js"></script>

    <!--=================================================-->
