    <style>
        .fade-loading {
            background: rgba(0, 0, 0, 0.75);
            display: flex;
            align-items: center;
            justify-content: center;
            position: fixed;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            z-index: 100000
        }

        .floating-container {
            z-index: 100001
        }

        .pad-5 {
            padding: 5px;
        }

        .mg-5-2 {
            margin: 5px 2px;
        }

        .mg-t-b-35-50 {
            margin-top: 35px;
            margin-bottom: 50px;
        }

        #cinema-form input, #cinema-form select {
            margin-bottom: 10px;
        }
    </style>
    @RenderSection("CSS", required: false)
</head>

<!--TIPS-->
<!--You may remove all ID or Class names which contain "demo-", they are only used for demonstration. -->
<body>
    <div id="container" class="effect aside-float aside-bright mainnav-lg">

