                                            <li><a href="/admin">Bảng tin</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="demo-pli-male"></i>
                                            <span class="menu-title">Bệnh nhân</span>
                                            <i class="arrow"></i>
                                        </a>

                                        <!--Submenu-->
