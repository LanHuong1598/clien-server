                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="demo-pli-file"></i>
                                            <span class="menu-title">Danh mục</span>
                                            <i class="arrow"></i>
                                        </a>

                                        <!--Submenu-->
                                        <ul class="@(ViewContext.RouteData.Values["controller"].ToString().ToLower().Equals("category")? "collapse in" : "collapse")">
                                            <li><a href="/admin/category">Danh sách</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="demo-pli-gear"></i>
                                            <span class="menu-title">Vật tư</span>
                                            <i class="arrow"></i>
                                        </a>

                                        <!--Submenu-->
                                        <ul class="@(ViewContext.RouteData.Values["controller"].ToString().ToLower().Equals("item")? "collapse in" : "collapse")">
                                            <li><a href="/admin/item">Danh sách</a></li>
                                            <li><a href="/admin/item/create">Thêm mới</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="demo-pli-repair"></i>
                                            <span class="menu-title">Thuốc</span>
                                            <i class="arrow"></i>
                                        </a>

                                        <!--Submenu-->
