    <!-- validate -->
    <script src="@baseURL/js/base/validate.js"></script>
    <!-- form-->
    <script src="@baseURL/js/base/form.js"></script>
    <!-- mess-->
    <script src="~/Areas/Admin/Content/js/base/mess.js"></script>
    <!-- config-->
    <script src="~/Areas/Admin/Content/js/base/config.js"></script>
    <!-- Include scripts -->
    @RenderSection("Scripts", required: false)
</body>
</html>